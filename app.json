{
  "expo": {
    "name": "solo-spark",
    "slug": "solo-spark",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
  
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.solospark",
      "googleServicesFile": "./google-services.json"
    }, "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.solosparktest", 
      "googleServicesFile": "./GoogleService-Info.plist" 
    } ,
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "@react-native-firebase/app"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4a998b40-cc61-4bc2-9606-3f188bb7c32b"
      }
    }
  }
}
